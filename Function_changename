import java.util.Arrays;

public class Function_changename {
    public static void main(String[] args) {
        // create a string
        int[] arr = { 1, 2, 3, 4, 5 };
        change(arr);
        System.out.println(Arrays.toString(arr));
    }

    static void change(int[] nums) {
        nums[0] = 99; // if you maske a change to the obj by referece variable same obj will change
    }
}
