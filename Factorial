import java.util.Scanner;

public class Factorial {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int num = in.nextInt();
        int i = 1;
        int Fact = 1;
        while (i <= num) {
            Fact = Fact * i;
            i++;
        }

        System.out.println(Fact);

    }
}
